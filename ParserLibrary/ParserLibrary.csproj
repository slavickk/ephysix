    <Compile Remove="OutputValues.cs" />
    <None Include="OutputValues.cs" />
    <Compile Remove="Filters.cs" />
    <None Include="Filters.cs" />
    <Compile Remove="CustomAttributes.cs" />
    <Content Include="Data\swagger.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <None Include="CustomAttributes.cs" />
  </ItemGroup>

