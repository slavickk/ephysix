<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://gitlab.com/SBPFraud/integration/integration-utility/-/packages/nuget</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\swagger.json" />
    <None Remove="nuget.config" />
    <Compile Remove="Components.cs" />
    <Compile Remove="DetectSensitive.cs" />
    <None Include="Components.cs" />
    <Compile Remove="Converters.cs" />
    <None Include="Converters.cs" />
    <Compile Remove="AbstrParser.cs" />
    <None Include="AbstrParser.cs" />
    <Compile Remove="AliasProducers.cs" />
