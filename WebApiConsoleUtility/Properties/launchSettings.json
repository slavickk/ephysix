{
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "YAML_PATH": ".\\Data\\ACS_TW.yml"
      }
    },
    "WebApiConsoleUtility": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "YAML_PATH": ".\\Data\\ACS_TW.yml"
      },
      "dotnetRunMessages": "true",
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "YAML_PATH": "/app/Data/ACS_TW.yml",
        "LOG_LEVEL": "Debug"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "DockerfileRunArguments": "-p 15001:15001 "
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:35047",
      "sslPort": 44352
    }
  }
}